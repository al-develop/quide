    ~1~ ApplicationName controls:Style @1@
    <controls:Style x:Key="ApplicationNamecontrols:Style" Selector="TextBlock">
        <controls:Setter Property="Foreground" Value="{StaticResource NavigationForegroundColorBrush}"/>

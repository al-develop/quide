    ~1~ ******************************* @1@
    ~1~ Login Container controls:Style @1@
    <controls:Style x:Key="LoginContainercontrols:Style" Selector="Border">
        <controls:Setter Property="Background" Value="#FF949290"/>
        <controls:Setter Property="Margin" Value="0,43,0,0"/>
        <controls:Setter Property="Height" Value="21"/>
        <controls:Setter Property="BorderThickness" Value="0,0,0,1"/>

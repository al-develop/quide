    ~1~ Links Border controls:Style @1@
    <controls:Style x:Key="LinksBordercontrols:Style" Selector="Border">
        <controls:Setter Property="Height" Value="42"/>
        <controls:Setter Property="Margin" Value="15,0,15,0"/>
        <controls:Setter Property="HorizontalAlignment" Value="Right"/>
    </controls:Style>

    ~1~ Links StackPanel controls:Style @1@
    <controls:Style x:Key="LinksStackPanelcontrols:Style" Selector="StackPanel">
        <controls:Setter Property="VerticalAlignment" Value="Center"/>
        <controls:Setter Property="HorizontalAlignment" Value="Left"/>
        <controls:Setter Property="Orientation" Value="Horizontal"/>
    </controls:Style>

    ~1~ Divider controls:Style @1@
    <controls:Style x:Key="Dividercontrols:Style" Selector="Rectangle">
        <controls:Setter Property="Fill" Value="#1FFFFFFF"/>
        <controls:Setter Property="Stroke" Value="Transparent"/>
        <controls:Setter Property="Width" Value="1"/>
        <controls:Setter Property="Margin" Value="2,4,2,4"/>
    </controls:Style>

    ~1~ Page ScrollViewer controls:Style @1@
    <controls:Style x:Key="PageScrollViewercontrols:Style" Selector="ScrollViewer">
        <controls:Setter Property="BorderBrush" Value="Transparent"/>
        <controls:Setter Property="BorderThickness" Value="0,1,0,1"/>
        <controls:Setter Property="VerticalScrollBarVisibility" Value="Auto"/>
        <controls:Setter Property="HorizontalScrollBarVisibility" Value="Auto"/>
    </controls:Style>

    ~1~ Content Panel controls:Style @1@
    <controls:Style x:Key="ContentStackPanelcontrols:Style" Selector="StackPanel">
        <controls:Setter Property="Margin" Value="35,0,35,0"/>
    </controls:Style>

    ~1~ Header Text controls:Style @1@
    <controls:Style x:Key="HeaderTextcontrols:Style" Selector="TextBlock">
        <controls:Setter Property="Foreground" Value="{StaticResource BodyTextColorBrush}"/>
        <controls:Setter Property="FontSize" Value="15"/>
        <controls:Setter Property="FontWeight" Value="Bold"/>
        <controls:Setter Property="TextWrapping" Value="Wrap"/>
        <controls:Setter Property="Margin" Value="0,15,0,4"/>
        <controls:Setter Property="HorizontalAlignment" Value="Left"/>
    </controls:Style>

    ~1~ Content Text controls:Style @1@
    <controls:Style x:Key="ContentTextcontrols:Style" Selector="TextBlock">
        <controls:Setter Property="Foreground" Value="{StaticResource BodyTextColorBrush}"/>
        <controls:Setter Property="FontSize" Value="12"/>
        <controls:Setter Property="TextWrapping" Value="Wrap"/>
        <controls:Setter Property="Margin" Value="0,2,0,2"/>
        <controls:Setter Property="HorizontalAlignment" Value="Left"/>
    </controls:Style>



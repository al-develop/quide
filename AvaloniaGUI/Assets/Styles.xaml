    ~1~ LayoutRoot Grid controls:Style @1@
    <controls:Style x:Key="LayoutRootGridcontrols:Style" Selector="Grid">
        <controls:Setter Property="Background" Value="#FFFFFFFF"/>
    </controls:Style>

    ~1~ Content Border controls:Style @1@
    <controls:Style x:Key="ContentBordercontrols:Style" Selector="Border">
        <controls:Setter Property="Background">
            <controls:Setter.Value>
                <controls:LinearGradientBrush EndPoint="0.5,0.045" StartPoint="0.5,0">
                    <controls:GradientStop Color="#6FCCCCCC"/>
                    <controls:GradientStop Color="#00CCCCCC" Offset="1"/>
                </controls:LinearGradientBrush>
            </controls:Setter.Value>
        </controls:Setter>
        <controls:Setter Property="BorderBrush" Value="#FFFFFFFF"/>
        <controls:Setter Property="BorderThickness" Value="0,1,0,0"/>
        <controls:Setter Property="Margin" Value="0,62,0,0"/>
        <controls:Setter Property="VerticalAlignment" Value="Stretch"/>
        <controls:Setter Property="HorizontalAlignment" Value="Stretch"/>
    </controls:Style>

    ~1~ Navigation Outer StackPanel controls:Style @1@
    <controls:Style x:Key="NavigationOuterGridcontrols:Style" Selector="Grid"/>

    ~1~ Navigation Grid controls:Style @1@
    <controls:Style x:Key="NavigationGridcontrols:Style" Selector="Grid">
        <controls:Setter Property="Background" Value="{StaticResource NavigationBackgroundColorBrush}"/>
        <controls:Setter Property="Height" Value="42"/>
        <controls:Setter Property="Margin" Value="0"/>
        <controls:Setter Property="VerticalAlignment" Value="Top"/>
    </controls:Style>

    ~1~ Branding Border controls:Style @1@
    <controls:Style x:Key="BrandingBordercontrols:Style" Selector="Border">
